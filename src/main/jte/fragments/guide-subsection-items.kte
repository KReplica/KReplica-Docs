@param section: io.availe.kreplicadocs.model.view.GuideNavSection
@param includeOverview: Boolean = false

@if(includeOverview)
    <li>
        <a href="#${section.id}">Overview</a>
    </li>
@endif
@for(subsection in section.subsections)
    <li>
        <a href="#${subsection.id}">${subsection.title}</a>
    </li>
@endfor